{
  "name": "bhilal",
  "version": "1.2.0",
  "description": "Un langage de programmation moderne avec outils de cybersécurité intégrés (Node.js + Go)",
  "main": "bhilal.js",
  "bin": {
    "bhilal": "./bin/bhilal",
    "bhilal-install": "./scripts/install.js"
  },
  "scripts": {
    "postinstall": "node scripts/install.js",
    "compile-tools": "node scripts/install.js",
    "test": "node bhilal.js test_rapide.bh"
