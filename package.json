  "keywords": [
    "language",
    "bhilal",
    "interpreter",
    "programming",
    "cybersecurity",
    "port-scanner",
    "penetration-testing",
    "french"
  ],
  "author": "Bhilal",
  "license": "MIT",
  "dependencies": {
    "readline-sync": "^1.4.10"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=14.0.0"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
